extends nav-bar

include utils

block name
    +csurf(csrfToken)
    div
        h1.search-results-title  Question Result Dummy!
    div.list-block-class
        div(id=`question-${questions.id}` class="question-block search-result-block")
            +question(questions)
            if user.id != currentUserId
                button.answer-button Answer
                div#answer-block
